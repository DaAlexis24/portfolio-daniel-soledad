---
import {Image} from 'astro:assets'
import logo from './images/logo.webp'

interface Props {
    class?: string;
    width: number;
    height: number;
}

const { class: className, width, height } = Astro.props;
---
<Image src={logo} alt="DS Logo" class={className} layout="constrained" width={width} height={height} fetchpriority="high" loading=eager />
